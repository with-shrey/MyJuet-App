<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.myjuet.com.myjuet">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AttendenceActivity"
            android:label="Attendence"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AttendenceDetailsActivity"
            android:label="Attendence Details"
            android:parentActivityName=".AttendenceActivity" />
        <activity
            android:name=".web.LoginWebkiosk"
            android:label="Login Details" />
    </application>

</manifest>